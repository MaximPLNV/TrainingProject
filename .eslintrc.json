{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 140
      }
    ],
    "linebreak-style": [
      "error", "windows"
    ]
  }
}
